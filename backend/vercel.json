{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/animeRoutes/(.*)",
            "dest": "https://anime-sphere-backend.vercel.app/api/animeRoutes"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}
